{
    "compilerOptions": {
        "target": "ES6",
        "baseUrl": "src",
        "jsx": "react",
        "paths": {
            "components:/*":[
                "./components/*"
            ],

            "containres:/*":[
                "./containres/*"
            ],

            "assets:/*":[
                "./assets/*"
            ],

            "commons:/*":[
                "./commons/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}